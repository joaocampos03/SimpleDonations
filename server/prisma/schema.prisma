generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produtos {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  custom_id   Int      @unique
  nome_prod   String
  desc_prod   String
  loc_prod    String
  data_doacao String
  img_prod    String
  status      String
}